#!/usr/bin/python
import sys

#read data input line by line
for line in sys.stdin:
	data = line.strip().split(",")
	
	if len(data)<3:
		continue
	print "{0},{1},{2}".format(data[0],data[0],data[1])
	for n in range(2,len(data)):
		#if data[0]=="Alivia":
		print "{0},{1},{2}".format(data[n],data[0],data[1])
		#print "{0},{1}".format(data[n],data[0])
